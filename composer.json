{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 9 projects with Composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Medard Rebero",
            "role": "Software Eng."
        }
    ],
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "d3": {
            "type": "package",
            "package": {
                "name": "d3/d3",
                "version": "v3.5.17",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/d3/d3.git",
                    "type": "git",
                    "reference": "tags/v3.5.17"
                }
            }
        },
        "highlightjs": {
            "type": "package",
            "package": {
                "name": "highlightjs/highlightjs",
                "version": "11.2.0",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/highlightjs/highlight.js.git",
                    "type": "git",
                    "reference": "tags/11.2.0"
                }
            }
        },
        "google-api-php-client": {
            "type": "package",
            "package": {
                "name": "googleapis/google-api-php-client",
                "version": "v2.12.6",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/googleapis/google-api-php-client.git",
                    "type": "git",
                    "reference": "tags/v2.12.6"
                }
            }
        }
    },
    "require": {
        "php": ">=8.2",
        "composer/installers": "^2.3",
        "cweagans/composer-patches": "^1.7",
        "d3/d3": "^3.5",
        "drupal/admin_toolbar": "^3.4",
        "drupal/bpmn_io": "^2.0",
        "drupal/config_split": "^2.0",
        "drupal/core-composer-scaffold": "^10.3",
        "drupal/core-project-message": "^10.3",
        "drupal/core-recommended": "^10.3",
        "drupal/devel_entity_updates": "^4.1",
        "drupal/eca": "^2.0",
        "drupal/email_registration": "^2.0@RC",
        "drupal/entity_update": "^3.0",
        "drupal/field_permissions": "^1.4",
        "drupal/geolocation": "^4.0@alpha",
        "drupal/gin": "^3.0@RC",
        "drupal/gin_login": "^2.1",
        "drupal/jsonapi_extras": "^3.26@beta",
        "drupal/jsonapi_resources": "^1.2",
        "drupal/module_filter": "^5.0",
        "drupal/openapi_jsonapi": "^3.0",
        "drupal/openapi_ui_redoc": "^1.0@RC",
        "drupal/pathauto": "^1.12",
        "drupal/restui": "^1.21",
        "drupal/simple_oauth": "^5.2",
        "drupal/symfony_mailer": "^1.4",
        "drupal/typed_data": "2.1.x-dev@dev",
        "drupal/views_bulk_edit": "^2.9",
        "drush/drush": "^12",
        "google/cloud-firestore": "^1.44",
        "google/cloud-tasks": "^1.14",
        "google/protobuf": "^4.27",
        "grpc/grpc": "^1.57",
        "highlightjs/highlightjs": "^11.2",
        "mautic/api-library": "^3.1.0",
        "stripe/stripe-php": "^13.18.0",
        "vlucas/phpdotenv": "^5.6",
        "webflo/drupal-finder": "^1.3",
        "wikimedia/composer-merge-plugin": "^2.1"
    },
    "require-dev": {
        "drupal/core-dev": "^10",
        "drupal/devel": "^5.2",
        "zaporylie/composer-drupal-optimizations": "^1.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "discard-changes": true,
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "plesk/composer-command-info": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "zaporylie/composer-drupal-optimizations": true,
            "drupal/core-project-message": true,
            "drupal/core-vendor-hardening": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "extra": {
        "merge-plugin": {
            "include": [
                "web/modules/custom/*/composer.json"
            ]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "web/drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/legacy-project template!               </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/installing-drupal",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
