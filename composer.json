{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 9 projects with Composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Medard Rebero",
            "role": "Software Eng."
        }
    ],
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "d3": {
            "type": "package",
            "package": {
                "name": "d3/d3",
                "version": "v3.5.17",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/d3/d3.git",
                    "type": "git",
                    "reference": "tags/v3.5.17"
                }
            }
        },
        "highlightjs": {
            "type": "package",
            "package": {
                "name": "highlightjs/highlightjs",
                "version": "11.2.0",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/highlightjs/highlight.js.git",
                    "type": "git",
                    "reference": "tags/11.2.0"
                }
            }
        },
        "google-api-php-client": {
            "type": "package",
            "package": {
                "name": "googleapis/google-api-php-client",
                "version": "v2.12.6",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/googleapis/google-api-php-client.git",
                    "type": "git",
                    "reference": "tags/v2.12.6"
                }
            }
        }
    },
    "require": {
        "php": ">=7.4",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "d3/d3": "^3.5",
        "drupal/admin_toolbar": "^3.0",
        "drupal/bpmn_io": "^1.1",
        "drupal/core-composer-scaffold": "^10",
        "drupal/core-project-message": "^10",
        "drupal/core-recommended": "^10",
        "drupal/devel_entity_updates": "^4.0",
        "drupal/eca": "^1.1",
        "drupal/email_registration": "^1.1",
        "drupal/entity_update": "^3.0",
        "drupal/field_permissions": "^1.1",
        "drupal/geolocation": "^3.7",
        "drupal/gin": "^3.0@beta",
        "drupal/gin_login": "^2.0",
        "drupal/jsonapi_extras": "^3.20",
        "drupal/jsonapi_resources": "^1.0@beta",
        "drupal/module_filter": "^4.0",
        "drupal/openapi_jsonapi": "^3.0",
        "drupal/openapi_ui_redoc": "^1.0@RC",
        "drupal/restui": "^1.20",
        "drupal/scheduler": "^2.0",
        "drupal/simple_oauth": "^5.2",
        "drupal/symfony_mailer": "^1.0@alpha",
        "drupal/typed_data": "^1.0@beta",
        "drush/drush": "^12",
        "highlightjs/highlightjs": "^11.2",
        "mautic/api-library": "^3.1.0",
        "stripe/stripe-php": "^9.2",
        "vlucas/phpdotenv": "^5.1",
        "webflo/drupal-finder": "^1.2"
    },
    "require-dev": {
        "drupal/core-dev": "^10",
        "drupal/devel": "^5.0@beta",
        "zaporylie/composer-drupal-optimizations": "^1.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "discard-changes": true,
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "plesk/composer-command-info": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "zaporylie/composer-drupal-optimizations": true,
            "drupal/core-project-message": true,
            "drupal/core-vendor-hardening": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
        }
    }
}
